Options All -Indexes
<FilesMatch "\.(inc.*)$">
    Order deny,allow
    Deny from all
</FilesMatch>

<FilesMatch "\.(htaccess|htpasswd|ini|log|sh|inc|bak|sql)$">
Order Allow,Deny
Deny from all
</FilesMatch>

#RewriteEngine on
#RewriteRule .* - [e=HTTP_AUTHORIZATION:%{HTTP:Authorization}]
#ReWriteRule ^v1\/autenticacao(\/[\d])?$ controllers/autenticacao.php$1 [NC]
#ReWriteRule ^v1\/usuario(\/*)?$ controllers/usuario.php$1 [NC]
#ReWriteRule ^v1\/usuario(\/*)?$ controllers/usuario.php$1 [NC]